@model List<Sklep__MVC_v3.Models.Brand>

@{
    ViewBag.Title = "Brands";
    Layout = "~/Areas/Admin/Views/Shared/_AdminLayoutPage.cshtml";
}

<h2>Brands</h2>

<table class="table">
    <tr>
        <th>Brand Id</th>
        <th>Brand Name</th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>@item.BrandID</td>
            <td>@item.BrandName</td>
        </tr>
    }


</table>